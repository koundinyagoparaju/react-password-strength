{
  "name": "@koundinyagoparaju/react-password-strength",
  "version": "1.0.3",
  "engines": {
    "node": ">=19.0.0"
  },
  "description": "A password strength indicator field for use in React projects. This doesn't use zxcvbn. Hence it is lightweight",
  "main": "dist/index.js",
  "scripts": {
    "build-module": "./node_modules/.bin/webpack --config=webpack.build.config.js",
    "build-prod": "npm run build-module -p & npm run build-universal-module -p",
    "build-universal-module": "./node_modules/.bin/webpack --config=webpack.universal.config.js --mode=production",
    "build-watch": "./node_modules/.bin/webpack --config=webpack.build.config.js --watch",
    "start": "npm run build-watch",
    "test": "./node_modules/karma/bin/karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koundinyagoparaju/react-password-strength.git"
  },
  "keywords": [
    "react",
    "password-strength"
  ],
  "author": "Koundinya Goparaju",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koundinyagoparaju/react-password-strength/issues"
  },
  "homepage": "https://github.com/koundinyagoparaju/react-password-strength#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "es6-shim": "^0.35.6",
    "jasmine": "^3.6.3",
    "karma": "^6.3.4",
    "karma-jasmine": "^4.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^5.0.0",
    "mini-css-extract-plugin": "^2.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^3.3.0",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.3.0",
    "webpack-dev-server": "^5.0.4",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  }
}
